      "source": "// Folder resource\\Section 12\\2 Source-code-Generics-Generics-Part-2\\Generics\\src\n// 4 printable files\n// (file list disabled)\n\n// resource\\Section 12\\2 Source-code-Generics-Generics-Part-2\\Generics\\src\\dev\\lpa\\BaseballTeam.java\n1\tpackage dev.lpa;\n2\t\n3\timport java.util.ArrayList;\n4\timport java.util.List;\n5\t\n6\tpublic class BaseballTeam {\n7\t\n8\t    private String teamName;\n9\t    private List<BaseballPlayer> teamMembers = new ArrayList<>();\n10\t    private int totalWins = 0;\n11\t    private int totalLosses = 0;\n12\t    private int totalTies = 0;\n13\t\n14\t    public BaseballTeam(String teamName) {\n15\t        this.teamName = teamName;\n16\t    }\n17\t\n18\t    public void addTeamMember(BaseballPlayer player) {\n19\t\n20\t        if (!teamMembers.contains(player)) {\n21\t            teamMembers.add(player);\n22\t        }\n23\t    }\n24\t\n25\t    public void listTeamMembers() {\n26\t\n27\t        System.out.println(teamName + \" Roster:\");\n28\t        System.out.println(teamMembers);\n29\t    }\n30\t\n31\t    public int ranking() {\n32\t        return (totalLosses * 2) + totalTies + 1;\n33\t    }\n34\t\n35\t    public String setScore(int ourScore, int theirScore) {\n36\t\n37\t        String message = \"lost to\";\n38\t        if (ourScore > theirScore) {\n39\t            totalWins++;\n40\t            message = \"beat\";\n41\t        } else if (ourScore == theirScore) {\n42\t            totalTies++;\n43\t            message = \"tied\";\n44\t        } else {\n45\t            totalLosses++;\n46\t        }\n47\t\n48\t        return message;\n49\t\n50\t    }\n51\t\n52\t    @Override\n53\t    public String toString() {\n54\t        return teamName + \" (Ranked \"  + ranking() + \")\";\n55\t    }\n56\t}\n57\t \n// resource\\Section 12\\2 Source-code-Generics-Generics-Part-2\\Generics\\src\\dev\\lpa\\Main.java\n1\tpackage dev.lpa;\n2\t\n3\tinterface Player {}\n4\trecord BaseballPlayer(String name, String position) implements Player {}\n5\trecord FootballPlayer(String name, String position) implements Player {}\n6\t\n7\tpublic class Main {\n8\t\n9\t    public static void main(String[] args) {\n10\t\n11\t        BaseballTeam phillies1 = new BaseballTeam(\"Philadelphia Phillies\");\n12\t        BaseballTeam astros1 = new BaseballTeam(\"Houston Astros\");\n13\t        scoreResult(phillies1, 3, astros1, 5);\n14\t\n15\t        SportsTeam phillies2 = new SportsTeam(\"Philadelphia Phillies\");\n16\t        SportsTeam astros2 = new SportsTeam(\"Houston Astros\");\n17\t        scoreResult(phillies2, 3, astros2, 5);\n18\t\n19\t        Team<BaseballPlayer> phillies = new Team<>(\"Philadelphia Phillies\");\n20\t        Team<BaseballPlayer> astros = new Team<>(\"Houston Astros\");\n21\t        scoreResult(phillies, 3, astros, 5);\n22\t\n23\t        var harper = new BaseballPlayer(\"B Harper\", \"Right Fielder\");\n24\t        var marsh = new BaseballPlayer(\"B Marsh\", \"Right Fielder\");\n25\t        phillies.addTeamMember(harper);\n26\t        phillies.addTeamMember(marsh);\n27\t        var guthrie = new BaseballPlayer(\"D Guthrie\", \"Center Fielder\");\n28\t        phillies.addTeamMember(guthrie);\n29\t        phillies.listTeamMembers();\n30\t\n31\t        SportsTeam afc1 = new SportsTeam (\"Adelaide Crows\");\n32\t        Team<FootballPlayer> afc = new Team<>(\"Adelaide Crows\");\n33\t        var tex = new FootballPlayer(\"Tex Walker\" , \"Centre half forward\");\n34\t        afc.addTeamMember(tex);\n35\t        var rory = new FootballPlayer(\"Rory Laird\", \"Midfield\");\n36\t        afc.addTeamMember(rory);\n37\t        afc.listTeamMembers();\n38\t    }\n39\t\n40\t    public static void scoreResult(BaseballTeam team1, int t1_score,\n41\t                                   BaseballTeam team2, int t2_score) {\n42\t\n43\t        String message = team1.setScore(t1_score, t2_score);\n44\t        team2.setScore(t2_score, t1_score);\n45\t        System.out.printf(\"%s %s %s %n\", team1, message, team2);\n46\t    }\n47\t\n48\t    public static void scoreResult(SportsTeam team1, int t1_score,\n49\t                                   SportsTeam team2, int t2_score) {\n50\t\n51\t        String message = team1.setScore(t1_score, t2_score);\n52\t        team2.setScore(t2_score, t1_score);\n53\t        System.out.printf(\"%s %s %s %n\", team1, message, team2);\n54\t    }\n55\t\n56\t    public static void scoreResult(Team team1, int t1_score,\n57\t                                   Team team2, int t2_score) {\n58\t\n59\t        String message = team1.setScore(t1_score, t2_score);\n60\t        team2.setScore(t2_score, t1_score);\n61\t        System.out.printf(\"%s %s %s %n\", team1, message, team2);\n62\t    }\n63\t}\n64\t \n// resource\\Section 12\\2 Source-code-Generics-Generics-Part-2\\Generics\\src\\dev\\lpa\\SportsTeam.java\n1\tpackage dev.lpa;\n2\t\n3\timport java.util.ArrayList;\n4\timport java.util.List;\n5\t\n6\tpublic class SportsTeam {\n7\t\n8\t    private String teamName;\n9\t    private List<Player> teamMembers = new ArrayList<>();\n10\t    private int totalWins = 0;\n11\t    private int totalLosses = 0;\n12\t    private int totalTies = 0;\n13\t\n14\t    public SportsTeam(String teamName) {\n15\t        this.teamName = teamName;\n16\t    }\n17\t\n18\t    public void addTeamMember(Player player) {\n19\t\n20\t        if (!teamMembers.contains(player)) {\n21\t            teamMembers.add(player);\n22\t        }\n23\t    }\n24\t\n25\t    public void listTeamMembers() {\n26\t\n27\t        System.out.println(teamName + \" Roster:\");\n28\t        System.out.println(teamMembers);\n29\t    }\n30\t\n31\t    public int ranking() {\n32\t        return (totalLosses * 2) + totalTies + 1;\n33\t    }\n34\t\n35\t    public String setScore(int ourScore, int theirScore) {\n36\t\n37\t        String message = \"lost to\";\n38\t        if (ourScore > theirScore) {\n39\t            totalWins++;\n40\t            message = \"beat\";\n41\t        } else if (ourScore == theirScore) {\n42\t            totalTies++;\n43\t            message = \"tied\";\n44\t        } else {\n45\t            totalLosses++;\n46\t        }\n47\t\n48\t        return message;\n49\t\n50\t    }\n51\t\n52\t    @Override\n53\t    public String toString() {\n54\t        return teamName + \" (Ranked \"  + ranking() + \")\";\n55\t    }\n56\t}\n57\t \n// resource\\Section 12\\2 Source-code-Generics-Generics-Part-2\\Generics\\src\\dev\\lpa\\Team.java\n1\tpackage dev.lpa;\n2\t\n3\timport java.util.ArrayList;\n4\timport java.util.List;\n5\t\n6\tpublic class Team<T> {\n7\t\n8\t    private String teamName;\n9\t    private List<T> teamMembers = new ArrayList<>();\n10\t    private int totalWins = 0;\n11\t    private int totalLosses = 0;\n12\t    private int totalTies = 0;\n13\t\n14\t    public Team(String teamName) {\n15\t        this.teamName = teamName;\n16\t    }\n17\t\n18\t    public void addTeamMember(T t) {\n19\t\n20\t        if (!teamMembers.contains(t)) {\n21\t            teamMembers.add(t);\n22\t        }\n23\t    }\n24\t\n25\t    public void listTeamMembers() {\n26\t\n27\t        System.out.println(teamName + \" Roster:\");\n28\t        System.out.println(teamMembers);\n29\t    }\n30\t\n31\t    public int ranking() {\n32\t        return (totalLosses * 2) + totalTies + 1;\n33\t    }\n34\t\n35\t    public String setScore(int ourScore, int theirScore) {\n36\t\n37\t        String message = \"lost to\";\n38\t        if (ourScore > theirScore) {\n39\t            totalWins++;\n40\t            message = \"beat\";\n41\t        } else if (ourScore == theirScore) {\n42\t            totalTies++;\n43\t            message = \"tied\";\n44\t        } else {\n45\t            totalLosses++;\n46\t        }\n47\t\n48\t        return message;\n49\t\n50\t    }\n51\t\n52\t    @Override\n53\t    public String toString() {\n54\t        return teamName + \" (Ranked \"  + ranking() + \")\";\n55\t    }\n56\t}\n57\t ",
