port java.util.List;\n5\t\n6\tpublic class MoreLists {\n7\t\n8\t    public static void main(String[] args) {\n9\t\n10\t        String[] items = {\"apples\", \"bananas\", \"milk\", \"eggs\"};\n11\t\n12\t        List<String> list = List.of(items);\n13\t        System.out.println(list);\n14\t\n15\t        ArrayList<String> groceries = new ArrayList<>(list);\n16\t        groceries.add(\"yogurt\");\n17\t        System.out.println(groceries);\n18\t\n19\t        ArrayList<String> nextList = new ArrayList<>(\n20\t                List.of(\"pickles\", \"mustard\", \"cheese\"));\n21\t        System.out.println(nextList);\n22\t\n23\t        groceries.addAll(nextList);\n24\t        System.out.println(groceries);\n25\t    }\n26\t}\n27\t ",
