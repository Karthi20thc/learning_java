ing s3 = list.removeLast(); // removes last element\n55\t        System.out.println(s3 + \" was removed\");\n56\t        // Queue/Deque poll methods\n57\t        String p1 = list.poll();  // removes first element\n58\t        System.out.println(p1 + \" was removed\");\n59\t        String p2 = list.pollFirst();  // removes first element\n60\t        System.out.println(p2 + \" was removed\");\n61\t        String p3 = list.pollLast();  // removes last element\n62\t        System.out.println(p3 + \" was removed\");\n63\t\n64\t        list.push(\"Sydney\");\n65\t        list.push(\"Brisbane\");\n66\t        list.push(\"Canberra\");\n67\t        System.out.println(list);\n68\t\n69\t        String p4 = list.pop();  // removes first element\n70\t        System.out.println(p4 + \" was removed\");\n71\t    }\n72\t\n73\t    private static void gettingElements(LinkedList<String> list) {\n74\t\n75\t        System.out.println(\"Retrieved Element = \" + list.get(4));\n76\t\n77\t        System.out.println(\"First Element = \" + list.getFirst());\n78\t        System.out.println(\"Last Element = \" + list.getLast());\n79\t\n80\t        System.out.println(\"Darwin is at position: \" + list.indexOf(\"Darwin\"));\n81\t        System.out.println(\"Melbourne is at position: \" +\n82\t                list.lastIndexOf(\"Melbourne\"));\n83\t        // Queue retrieval method\n84\t        System.out.println(\"Element from element() = \" + list.element());\n85\t        // Stack retrieval methods\n86\t        System.out.println(\"Element from peek() = \" + list.peek());\n87\t        System.out.println(\"Element from peekFirst() = \" + list.peekFirst());\n88\t        System.out.println(\"Element from peekLast() = \" + list.peekLast());\n89\t    }\n90\t\n91\t    public static void printItinerary(LinkedList<String> list) {\n92\t\n93\t        System.out.println(\"Trip starts at \" + list.getFirst());\n94\t        for (int i = 1; i < list.size(); i++) {\n95\t            System.out.println(\"--> From: \" + list.get(i - 1) + \" to \" + list.get(i));\n96\t        }\n97\t        System.out.println(\"Trip ends at \" + list.getLast());\n98\t    }\n99\t\n100\t    public static void printItinerary2(LinkedList<String> list) {\n101\t\n102\t        System.out.println(\"Trip starts at \" + list.getFirst());\n103\t        String previousTown = list.getFirst();\n104\t        for (String town : list) {\n105\t            System.out.println(\"--> From: \" + previousTown + \" to \" + town);\n106\t            previousTown = town;\n107\t        }\n108\t\n109\t        System.out.println(\"Trip ends at \" + list.getLast());\n110\t    }\n111\t\n112\t    public static void printItinerary3(LinkedList<String> list) {\n113\t\n114\t        System.out.println(\"Trip starts at \" + list.getFirst());\n115\t        String previousTown = list.getFirst();\n116\t        ListIterator<String> iterator = list.listIterator(1);\n117\t        while (iterator.hasNext()) {\n118\t            var town = iterator.next();\n119\t            System.out.println(\"--> From: \" + previousTown + \" to \" + town);\n120\t            previousTown = town;\n121\t        }\n122\t\n123\t        System.out.println(\"Trip ends at \" + list.getLast());\n124\t    }\n125\t}\n126\t\n127\t ",
